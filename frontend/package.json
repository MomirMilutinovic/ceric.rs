{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "containerize": "docker build --network=host -t angularapp . && docker run --name angular -p 8787:8787 -v ./uploads:/usr/share/nginx/uploads --network=\"host\" angularapp",
    "recontainerize": "docker rm angular && docker build  --network=host -t angularapp . && docker run --name angular -p 8787:8787 -v ./uploads:/usr/share/nginx/uploads --network=\"host\" angularapp",
    "deploy": "ng build --configuration development && docker cp dist/frontend/browser/. angular:/usr/share/nginx/html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.2.11",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.2.11",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@material/chips": "^14.0.0",
    "@stomp/stompjs": "^7.0.0",
    "chart.js": "^4.4.6",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "ng2-charts": "^5.0.4",
    "rxjs": "~7.8.0",
    "sockjs-client": "^1.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular/cli": "^18.0.6",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.14",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}
